<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.LevelSelectController">
   <children>
      <ImageView fitHeight="1080.0" fitWidth="1920.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../imgs/bg2.jpg" />
         </image>
      </ImageView>
      <Label layoutX="757.0" layoutY="112.0" text="Select Level">
         <font>
            <Font name="Arial Black" size="53.0" />
         </font>
      </Label>
      <Button layoutX="68.0" layoutY="912.0" mnemonicParsing="false" onMouseClicked="#backtoMenu" text="BACK">
         <font>
            <Font name="Arial Black" size="36.0" />
         </font>
      </Button>
      <AnchorPane fx:id="levels" layoutX="230.0" layoutY="285.0">
         <children>
            <ImageView fitHeight="250.0" fitWidth="212.0" layoutY="106.0" onMouseClicked="#loadLevel1" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imgs/levelIcons/lev1.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="250.0" fitWidth="212.0" layoutX="318.0" layoutY="2.0" onMouseClicked="#loadLevel2" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imgs/levelIcons/lev2.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="250.0" fitWidth="212.0" layoutX="639.0" layoutY="99.0" onMouseClicked="#loadLevel3" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imgs/levelIcons/lev3.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="250.0" fitWidth="212.0" layoutX="977.0" onMouseClicked="#loadLevel4" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imgs/levelIcons/lev4.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="250.0" fitWidth="212.0" layoutX="1308.0" layoutY="105.0" onMouseClicked="#loadLevel5" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imgs/levelIcons/lev5.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
